{
  "version": 2,
  "templateParts": [],
  "customTemplates": [],
  "settings": {
    "color": {
      "palette": [
        {
          "name": "Base",
          "slug": "base",
          "color": "#FFFFFF"
        },
        {
          "name": "Typography",
          "slug": "typography",
          "color": "#354555"
        },
        {
          "name": "Brand",
          "slug": "brand",
          "color": "#74AF27"
        },
        {
          "name": "Main",
          "slug": "main",
          "color": "#354555"
        },
        {
          "name": "MainContrast",
          "slug": "main-contrast",
          "color": "#F9F9F9"
        }
      ]
    },
    "custom": {
      "fontWeight": {
        "thin": 100,
        "extra-light": 200,
        "light": 300,
        "regular": 400,
        "medium": 500,
        "semi-bold": 600,
        "bold": 700,
        "extra-bold": 800,
        "black": 900
      },
      "lineHeight": {
        "one": 1,
        "heading": 1.2,
        "body": 1.5
      },
      "spacing": {
        "sminimal": "2px",
        "sxxs": "10px",
        "sxs": "20px",
        "sm": "25px",
        "sl": "30px",
        "sxl": "40px",
        "sxxl": "60px",
        "sspacer": "100px"
      }
    },
    "layout": {
      "contentSize": "860px",
      "wideSize": "100vw"
    },
    "spacing": {
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "'Adelle', serif",
          "name": "Adelle",
          "slug": "adelle"
        }
      ],
      "fontSizes": [
        {
          "fluid": false,
          "name": "xSmall",
          "slug": "x-small",
          "size": "0.88rem"
        },
        {
          "fluid": false,
          "name": "Small",
          "slug": "small",
          "size": "1rem"
        },
        {
          "fluid": false,
          "name": "Medium",
          "slug": "medium",
          "size": "1.33rem"
        },
        {
          "fluid": {
            "min": "1.33rem",
            "max": "1.55rem"
          },
          "name": "Large",
          "slug": "large",
          "size": "1.55rem"
        },
        {
          "fluid": {
            "min": "1.55rem",
            "max": "1.66rem"
          },
          "name": "xLarge",
          "slug": "x-large",
          "size": "1.66rem"
        },
        {
          "fluid": {
            "min": "1.66rem",
            "max": "2.77rem"
          },
          "name": "xxLarge",
          "slug": "xx-large",
          "size": "2.77rem"
        },
        {
          "fluid": {
            "min": "2.77rem",
            "max": "3.27rem"
          },
          "name": "xxxLarge",
          "slug": "xxx-large",
          "size": "3.27rem"
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/button": {
        "border": {
          "radius": "0px"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--brand)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sxxs)",
            "right": "var(--wp--custom--spacing--sxl)",
            "bottom": "var(--wp--custom--spacing--sxxs)",
            "left": "var(--wp--custom--spacing--sxl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--adelle)",
          "fontSize": "var(--wp--preset--font-size--small)",
          "fontWeight": "var(--wp--custom--font-weight--semi-bold)"
        }
      },
      "core/calendar": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/code": {
        "border": {
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--main-contrast)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--monospace)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/columns": {
        "spacing": {
          "margin": {
            "bottom": "0px"
          }
        }
      },
      "core/cover": {
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/group": {
        "spacing": {
          "padding": "0px"
        }
      },
      "core/heading": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sxs)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--adelle)",
          "fontWeight": "var(--wp--custom--font-weight--bold)"
        },
        "color": {
          "text": "var(--wp--preset--color--primary)"
        }
      },
      "core/image": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "fontWeight": "var(--wp--custom--font-weight--medium)"
        },
        "color": {
          "text": "var(--wp--preset--color--typography)"
        }
      },
      "core/paragraph": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/post-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xxx-large)"
        }
      },
      "core/preformatted": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "radius": "0"
        },
        "color": {
          "background": "var(--wp--preset--color--main)",
          "text": "var(--wp--preset--color--main-contrast)"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sm)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sm)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--adelle)",
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/pullquote": {
        "border": {
          "color": "var(--wp--preset--color--main)",
          "style": "solid",
          "width": "1px"
        },
        "spacing": {
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "var(--wp--custom--spacing--sl)",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "var(--wp--custom--spacing--sl)"
          }
        }
      },
      "core/quote": {
        "border": {
          "color": "var(--wp--preset--color--secondary)",
          "style": "solid !important",
          "width": "1px !important"
        },
        "spacing": {
          "margin": {
            "bottom": "35px"
          },
          "padding": {
            "top": "var(--wp--custom--spacing--sl)",
            "right": "clamp(var(--wp--custom--spacing--sxs), 3vw, var(--wp--custom--spacing--sxl)) !important",
            "bottom": "var(--wp--custom--spacing--sl)",
            "left": "clamp(var(--wp--custom--spacing--sxs), 3vw, var(--wp--custom--spacing--sxl)) !important"
          }
        },
        "typography": {
          "lineHeight": "var(--wp--custom--line-height--body)",
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "core/list": {
        "color": {
          "text": "var(--wp--preset--color--typography)"
        }
      },
      "core/spacer": {
        "spacing": {
          "margin": {
            "top": "0 !important"
          }
        }
      },
      "core/separator": {
        "color": {
          "text": "var(--wp--preset--color--main)"
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "core/site-tagline": {
        "spacing": {
          "margin": {
            "bottom": "var(--wp--custom--spacing--sxs)"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--one)",
          "textTransform": "uppercase"
        }
      },
      "core/table": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "core/template-part": {
        "spacing": {
          "margin": {
            "top": "0px"
          }
        }
      }
    },
    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--typography)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "lineHeight": "var(--wp--custom--line-height--heading)",
          "fontWeight": "var(--wp--custom--font-weight--bold)"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "var(--wp--custom--font-weight--bold)",
          "lineHeight": "var(--wp--custom--line-height--heading)"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-large)"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)"
        }
      },
      "h5": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--small)"
        }
      },
      "h6": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--x-small)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--brand)"
        },
        "typography": {
          "textDecoration": "none"
        }
      }
    },
    "spacing": {
      "blockGap": "var(--wp--custom--spacing--sl)",
      "padding": {
        "top": "0px",
        "right": "0px",
        "bottom": "0px",
        "left": "0px"
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--adelle)",
      "fontSize": "var(--wp--preset--font-size--small)",
      "fontWeight": "var(--wp--custom--font-weight--regular)",
      "lineHeight": "var(--wp--custom--line-height--body)"
    }
  }
}
